/* Fix modal backdrop for smaller devices */
@media (max-width: 679px) {
  .active .modal-backdrop-bg {
    opacity: .5;
  }
  .modal-backdrop-bg {
    -webkit-transition: opacity 300ms ease-in-out;
    transition: opacity 300ms ease-in-out;
    background-color: #000;
    opacity: 0;
  }
}
a:link {
    text-decoration: none;
}


.sched-content-inner {
  margin-top: 20px;
  position: relative;
  background-color:black;
}

.sched-container-header {
  margin: 0 10px;
}

.sched-container-dates {
  line-height: 35px;
  color: #333;
  font-size: 18px;
}


.sched-container-top {
  clear: both;
  height: 1px;
}

#agenda_time {
  border: 1px solid;
  clear: left;
  color: #999;
  float: left;
  margin: 16px 0 0;
  text-align: right;
  width: 55px;
  zoom: 1;
  font-size: 14px;
}

#agenda_duration {
  clear: left;
  color: #999;
  float: left;
  margin: 16px 0 0;
  text-align: right;
  width: 55px;
  zoom: 1;
  font-size: 16px;
}

.sched-container {
  border-left: 1px solid rgba(0,0,0,.10);
  margin: -1px 0 0 68px;
  overflow: auto;
}

#sched-page-home .sched-container-inner {
  padding-bottom: 5px;
}

.sched-container-inner {
  padding: 10px 0 0 15px;
  position: relative;
}

.event {
  border-bottom: 1px solid rgba(0,0,0,0.15);
  border-left: 1px solid transparent;
  border-radius: 4px;
  border-right: 1px solid rgba(0,0,0,0.15);
  border-top: 1px solid transparent;
  cursor: pointer;
  display: block;
  float: left;
  margin: 0 12px 12px 0;
  position: relative;
  text-shadow: 1px 1px 0 rgba(255,255,255,.15);
  box-shadow: 0 1px 2px rgba(0,0,0,0.25);
  background-color: #DCDCDC;
  padding-left: 32px;
  max-width: 100%;
  width: 100%;
}

#sched-content .ev-save {
  background: url(//cdn.schd.ws/common/img/sched-open.png) 5px 5px no-repeat;
  /* background: url(//cdn.schd.ws/common/star-white.png) 3px 2px no-repeat; */
  border-radius: 3px;
  display: block;
  left: 0;
  line-height: 24px;
  margin: 1px;
  position: absolute;
  top: 0;
  width: 24px;
  background-size: 16px 16px;
  background-image: url(//cdn.schd.ws/common/img/sched-unchecked4.png);
  background-position: 4px 4px;
  opacity: .8;
}

#schedorg .event a {
  color: #000;
}

.event-loggedin .name {
  margin-left: 27px;
}

.event .name {
  float: left;
  line-height: 27px;
}

.event a {
  margin-left: 26px;
  border-left: 1px solid rgba(0,0,0,0.15);
}

.event a {
  border-bottom-right-radius: 3px;
  border-top-right-radius: 3px;
  margin: 0;
  padding: 0 5px;
  color:#000;
}

.event .vs {
  color: #444;
  display: none;
  margin-left: 5px;
}

ul.dropdown-menu a {
  margin: 5px;
}

.event.lunch , .event.review , .event.qa {
  background-color: #9EDF7D;
  width:100%;
}

.event.lunch .name , .event.review .name ,.event.qa .name, .event.opening .name{
  color:#fff !important;
}

.event.lunch a ,.event.review a , .event.qa a{
  border-left: 1px solid rgba(255,255,255,0.15);
}

.event.review {
  background-color: orange;
}
.event.opening { 
  background-color: red;
}
.event.qa {
  background-color: #188BBB;
}
@media (min-width: 1200px) {
  #sched-content-inner h3 {
    font-size: 14px;
    margin-top: 17px;
    width: 70px;
  }
  .sched-container {
    margin-left: 85px;
  }
  .event {
    font-size: 16px;
  }
  #sched-content .ev-save {
    background: url(//cdn.schd.ws/common/img/sched-unchecked5.png) 4px 4px no-repeat;
    background-size: 21px 21px;
  }
  #sched-content .ev-save {
    width: 30px;
    line-height: 30px;
  }
  .event-loggedin .name {
    margin-left: 32px;
  }
  .event .name {
    line-height: 32px;
  }
  .event a {
    padding: 0 9px;
  }
}

#scrollToTop{
  height: 40px;
  width: 60px;
 
  position: fixed;
  bottom: 0px;
  right: 0px;
  border-radius: 10px 0 0 0;
  
  z-index: 1000;
  display:none;
  background:rgba(232, 98, 86,0.9) url('../img/arrow.svg') no-repeat center 50%;
}
